  bottom: "conv8_1"
  top: "conv8_2"
  name: "conv8_2"
  type: "Convolution"
  convolution_param {
    num_output: 512 
    pad:1 
    kernel_size: 3
    stride: 2
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  bottom: "conv8_2"
  top: "conv8_2"
  name: "relu8_2"
  type: "ReLU"
}

#conv9_1
layer {
  bottom: "conv8_2"
  top: "conv9_1"
