    stride: 2
  }
}
layer {
  bottom: "pool4"
  top: "conv5_1"
  name: "conv5_1"
  type: "Convolution"
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv5_1"
  top: "conv5_1"
  name: "relu5_1"
  type: "ReLU"
}
layer {
  bottom: "conv5_1"
  top: "conv5_2"
  name: "conv5_2"
  type: "Convolution"
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv5_2"
  top: "conv5_2"
  name: "relu5_2"
  type: "ReLU"
}
layer {
  bottom: "conv5_2"
  top: "conv5_3"
  name: "conv5_3"
  type: "Convolution"
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv5_3"
  top: "conv5_3"
  name: "relu5_3"
  type: "ReLU"
}

#modify pool5 from 2*2-s2 to 3*3-s1
layer {
  bottom: "conv5_3"
  top: "pool5"
  name: "pool5"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 3 
    stride: 1 
    pad: 1
  }
}

#modify from fc6, fc7 to conv layers
# conv6
layer {
  bottom: "pool5"
  top: "conv6"
  name: "conv6"
  type: "Convolution"
  convolution_param {
    num_output: 1024
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv6"
  top: "conv6"
  name: "relu6"
  type: "ReLU"
}

#conv7
layer {
  bottom: "conv6"
  top: "conv7"
  name: "conv7"
  type: "Convolution"
  convolution_param {
    num_output: 1024
    pad: 0
    kernel_size: 1
  }
}
layer {
  bottom: "conv7"
  top: "conv7"
  name: "relu7"
  type: "ReLU"
}

#conv8_1,layers below this are newly added
layer {
  bottom: "conv7"
  top: "conv8_1"
  name: "conv8_1"
  type: "Convolution"
  convolution_param {
    num_output: 256 
    pad: 0
    kernel_size: 1
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  bottom: "conv8_1"
  top: "conv8_1"
  name: "relu8_1"
  type: "ReLU"
}

#conv8_2
